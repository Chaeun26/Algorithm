    "num=int(input())\n",
    "arr=[[1]*10 for _ in range(0,num)]\n",
    "\n",
    "for i in range(1,num):\n",
    "    for j in range(1,10):\n",
    "        arr[i][j]=arr[i-1][j]+arr[i][j-1]\n",
    "\n",
    "print(sum(arr[num-1])%10007)"
