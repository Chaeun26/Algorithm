    "num=int(input())\n",
    "\n",
    "score=[]\n",
    "hacker=0\n",
    "n=1\n",
    "\n",
    "for _ in range(num):\n",
    "    score.append(int(input()))\n",
    "\n",
    "for honor in sorted(score):\n",
    "    if honor >= n:\n",
    "        hacker += honor-n\n",
    "        n+=1\n",
    "\n",
    "print(hacker)"
